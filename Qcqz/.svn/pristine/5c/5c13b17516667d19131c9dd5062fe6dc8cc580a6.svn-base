<%@ page language="java" pageEncoding="UTF-8"%>
<div style="padding: 5px;">
	<table id="win" width="320px" height="200px;" border="1"
		style="float:left;border-collapse:collapse;margin:5px;border:1px solid #95B8E7;">
		
	</table>
	<div id="showImage"
		style="border:1px solid #95B8E7;border-radius:5px;float:right;width:150px;height:150px;margin:5px;margin-top:35px;">

	</div>
	<div
		style="border:0px solid;float:left;width:320px;height:100px;margin:5px;">
		<input class="easyui-textbox" name="message"
			data-options="multiline:true,prompt:'测评意见'"
			style="float:left;width:320px;height:100px;margin:5px;"></input>
	</div>
</div>